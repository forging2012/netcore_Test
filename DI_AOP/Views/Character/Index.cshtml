@model IEnumerable<Character>
@{
    ViewData["Title"] = "Characters";
}
<h1>Characters</h1>
<div class="row">
    <div class="col-md-12">
        <table>
            <tr>
                <th>Name</th>
            </tr>
            @foreach (var c in Model)
            {
                <tr>
                    <td>@c.Name</td>
                </tr>
            }
        </table>
    </div>
</div>